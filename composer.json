    "ext-dom": "*",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-bcmath": "*",
    "ext-libxml": "*",
    "doctrine/dbal": "^4.0",
    "doctrine/migrations": "^3.7",
    "guzzlehttp/guzzle": "^7.4",
    "league/csv": "^9.0",
    "league/flysystem": "^3.24",
    "moneyphp/money": "^4.0",
    "olvlvl/composer-attribute-collector": "^2.0",
    "php-di/php-di": "^7",
    "symfony/console": "^7.0",
    "thecodingmachine/safe": "^2.5"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.49"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "olvlvl/composer-attribute-collector": true
    }
